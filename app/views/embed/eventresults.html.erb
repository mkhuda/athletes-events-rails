<ul class="list-group">
	<li class="list-group-item disabled">
	<div class="row">
			<div class="name col-md-12"><p class="text-center"><%= @event.name %> - Results</p></div>
	</div>
	</li>
	<li class="list-group-item active">
	<div class="row">
			<div class="name col-md-5">Name</div>
			<div class="city col-md-3">City</div>
			<div class="city col-md-2">Position</div>
			<div class="city col-md-2">Point</div>
	</div>
	</li>
	<% @fetch.each_with_index do |a, index| %>
	
	<li class="list-group-item">
	<div class="row">
		<% if @event.category == "Individual" %>
			<div class="name col-md-5"><%= a.athlete.name %></div>
			<div class="city col-md-3"><%= a.athlete.city %></div>
			<div class="city col-md-2"><%= a.position %></div>
			<div class="city col-md-2"><%= a.point %></div>
		<% else %>
			<div class="name col-md-5"><%= a.team.name %></div>
			<div class="city col-md-3"><%= a.team.city %></div>
			<div class="city col-md-2"><%= a.position %></div>
			<div class="city col-md-2"><%= a.point %></div>
		<% end %>
	</div>
	</li>
	<% end %>
	<li class="list-group-item disabled">
	<div class="row">
			<div class="name col-md-12">
			<p class="text-right pull-left">ICF 2016</p>
			<p class="text-right pull-right"><i class="fa fa-calendar"></i> <strong><%= @event.startdate.strftime('%d %B %Y') %></strong> - <strong><%= @event.finishdate.strftime('%d %B %Y') %></strong></p>
			</div>
	</div>
	</li>
	</ul>